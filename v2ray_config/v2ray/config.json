{
  "log": {
    "access": "/var/log/v2ray/access.log",
    "error": "/var/log/v2ray/error.log",
    "loglevel": "info"
  },
  "inbound": {
    "port": 10086,
    "listen": "0.0.0.0",
    "protocol": "vmess",
    "settings": {
      "clients": [
        {
          "id": "__ClientId__",
          "level": 1,
          "alterId": 0,
          "security": "none"
        }
      ]
    },
    "streamSettings": {
      "network": "ws",
      "security": "auto",
      "wsSettings": {
        "path": "__WsPath__"
      }
    }
  },
  "outbound": {
    "protocol": "freedom",
    "settings": {}
  }
}
